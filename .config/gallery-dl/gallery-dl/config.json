{
  "base-directory": "~/Pictures/Gallery",
  "cookies": [
    "firefox"
  ],
  "archive": "~/.config/gallery-dl/archive.sqlite3",
  "extractor": {
    "danbooru": {
      "directory": [
        "danbooru"
      ],
      "filename": "{id}.{extension}",
      "skip": true
    },
    "gelbooru": {
      "directory": [
        "gelbooru"
      ],
      "filename": "{id}.{extension}",
      "skip": true
    },
    "rule34": {
      "directory": [
        "rule34"
      ],
      "filename": "{id}.{extension}",
      "skip": true
    },
    "e621": {
      "directory": [
        "e621"
      ],
      "filename": "{id}.{extension}",
      "skip": true
    },
    "wallhaven": {
      "directory": [
        "wallpapers"
      ],
      "filename": "{id}.{extension}",
      "skip": true
    },
    "ehentai": {
      "directory": [
        "ehentai",
        "{gallery_id}"
      ],
      "filename": "{gallery_id}_{num:>02}.{extension}",
      "skip": true
    },
    "nhentai": {
      "directory": [
        "nhentai",
        "{gallery_id} - {title}"
      ],
      "filename": "{gallery_id}_{num:>02}.{extension}",
      "skip": true
    },
    "generic": {
      "directory": [
        "other"
      ],
      "filename": "{id}_{title}_{date:%Y-%m-%d}.{extension}",
      "skip": true
    }
  },
  "downloader": {
    "retries": 3,
    "timeout": 10.0
  },
  "output": {
    "mode": "color",
    "skip": true
  }
}